<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<h2>ALL POST!</h2>
<%= link_to 'New Post!', new_post_path %>
<table>
  <tr>
    <th>title</th>
    <th>text</th>
    <th colspan="3"></th>
  </tr>
  <% @posts.each do |post| %>
  <tr>
    <td><%= post.rname %></td>
    <td><img src="<%= "#{post.rimage}" %>"></td>
  </tr>
  <% end %>
</table>
